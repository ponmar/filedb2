<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="FileDBApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:FileDBApp"
    xmlns:view="clr-namespace:FileDBApp.View"
    Title="FileDBApp"
    Shell.FlyoutBehavior="Flyout">

    <FlyoutItem Title="Birthdays">
        <ShellContent ContentTemplate="{DataTemplate view:BirthdaysPage}" />
    </FlyoutItem>
    <FlyoutItem Title="Rest in Peace">
        <ShellContent ContentTemplate="{DataTemplate view:RipPage}" />
    </FlyoutItem>
    
</Shell>
